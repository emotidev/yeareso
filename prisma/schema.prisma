// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("COCKROACHDB")
}

model Resolution {
  email      String  @unique @id
  phoneNumber String?
}
